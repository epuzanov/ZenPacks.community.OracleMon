<object id='/zport/dmd/Devices/Server/rrdTemplates/OracleTablespace' module='Products.ZenModel.RRDTemplate' class='RRDTemplate'>
<property type="text" id="description" mode="w" >
Oracle Tablespace monitoring template
</property>
<property type="string" id="targetPythonClass" mode="w" >
ZenPacks.community.OracleMon.OracleTablespace
</property>
<tomanycont id='datasources'>
<object id='sizeUsed' module='ZenPacks.community.SQLDataSource.datasources.SQLDataSource' class='SQLDataSource'>
<property select_variable="sourcetypes" type="selection" id="sourcetype" mode="w" >
SQL
</property>
<property type="boolean" id="enabled" mode="w" >
True
</property>
<property type="string" id="eventClass" mode="w" >
/Status/OracleTablespace
</property>
<property type="int" id="severity" mode="w" >
5
</property>
<property type="int" id="cycletime" mode="w" >
300
</property>
<property type="string" id="cs" mode="w" >
${here/zOracleConnectionString}
</property>
<property type="string" id="sql" mode="w" >
SELECT    b.BLOCKS/a.MAXBLOCKS percentUsed,
               b.BLOCKS sizeUsed,
               a.BYTES totalBytes,
               s.STATUS status,
               TABLESPACE_NAME
FROM
        (
                SELECT     TABLESPACE_NAME,
                                decode( STATUS,
                                     'ONLINE', 2,
                                     'READ ONLY', 3,
                                     'OFFLINE', 4,
                                     1) STATUS
                FROM        dba_tablespaces
        ) s
LEFT JOIN
        (
                SELECT     TABLESPACE_NAME,
                                sum(BYTES) BYTES,
                                sum(decode(AUTOEXTENSIBLE, 'YES', MAXBLOCKS, BLOCKS)) MAXBLOCKS
                FROM        dba_data_files
                GROUP BY TABLESPACE_NAME
        ) a
USING (TABLESPACE_NAME)
LEFT JOIN
        (
                SELECT     TABLESPACE_NAME,
                                sum(BLOCKS) BLOCKS
                FROM        dba_segments
                GROUP BY TABLESPACE_NAME
        ) b
USING (TABLESPACE_NAME)
WHERE      TABLESPACE_NAME='${here/dbname}'
</property>
